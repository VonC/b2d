#!/bin/sh

run() {
	set -e
	pcrt=$(readlink -f ./apache/crts)
	if [ ! -e tests/blessed/gitolite-admin ]; then
		git -c http.https://localhost:6443/.sslCAInfo=${dirp0}/apache/crts -c credential.helper=passwd clone https://gitoliteadm@localhost:6443/hgit/gitolite-admin tests/blessed/gitolite-admin
	fi
	. tests/utils
	cd tests/blessed/gitolite-admin
	git_config 6443 "${pcrt}" gitoliteadm
}
